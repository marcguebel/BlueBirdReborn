{
    "manifest_version": 3,
    "name": "Blue bird reborn",
    "version": "1.0.1",
    "description": "Revive the blue bird",
    "content_scripts": [
        {
            "matches": ["https://twitter.com/*"],
            "js": ["script.js"]
        }
    ],
    "web_accessible_resources": [
        {
            "resources": [ "favicon.ico" ],
            "matches": [ "https://twitter.com/*" ]
        }
    ],
    "icons": {
        "16": "icon16.png",
        "32": "icon32.png",
        "48": "icon48.png",
        "128": "icon128.png"
    }
}  